{
  "compilerOptions": {
    "target": "ESNext",
    "moduleResolution": "bundler",
    "module": "ESNext",
    "emitDeclarationOnly": true,
    "declarationMap": true,
    "jsx": "react-jsx",
    "composite": true,
    "outDir": "dist",
    "rootDir": "src",
    "baseUrl": "./",
    "types": [
      "next"
    ]
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "dist"
  ]
}
